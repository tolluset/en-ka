{
  "name": "en-ka",
  "version": "1.2.0",
  "description": "Smart English to Katakana CLI converter with intelligent search modes",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "en-ka": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "lint": "eslint src --ext .ts",
    "lint-fix": "eslint src --ext .ts --fix",
    "lint:type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm run lint:type-check && pnpm run build",
    "release": "release-it",
    "release:dry": "release-it --dry-run"
  },
  "keywords": [
    "japanese",
    "katakana",
    "cli",
    "dictionary",
    "translation"
  ],
  "author": "en-ka",
  "license": "CC-BY-SA-4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tolluset/en-ka.git"
  },
  "homepage": "https://github.com/tolluset/en-ka#readme",
  "bugs": {
    "url": "https://github.com/tolluset/en-ka/issues"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "fuzzy": "^0.1.3"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "release-it": "^19.0.5",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@10.12.4"
}
